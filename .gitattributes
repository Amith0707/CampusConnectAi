* text=auto

# Explicitly set line endings for common file types
*.js     text eol=lf
*.jsx    text eol=lf
*.ts     text eol=lf
*.tsx    text eol=lf
*.css    text eol=lf
*.html   text eol=lf
*.json   text eol=lf
*.md     text eol=lf
*.sh     text eol=lf
*.py     text eol=lf
*.ipynb  text eol=lf
*.yml    text eol=lf
*.yaml   text eol=lf
